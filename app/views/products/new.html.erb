<% provide(:title, '入荷フォーム') %>
<h1>入荷フォーム</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: [item, product], local: true) do |f| %>
    
      <%= f.label :lot class: "label-#{yield(:class_text)}" %>
      <%= f.number_field :lot class: "form-control" %>
      
    <% end %>
  </div>
</div>